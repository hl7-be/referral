      <sliceName value="Annotation" />
    </element>
    <element id="Extension.extension:Annotation.url">
      <path value="Extension.extension.url" />
      <fixedUri value="Annotation" />
    </element>
    <element id="Extension.extension:Annotation.value[x]">
      <path value="Extension.extension.value[x]" />
