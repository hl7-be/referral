  <title value="BeContraindication" />
  <status value="draft" />
  <description value="Contra indication in a prescription referral for diagnostic imaging." />
  <fhirVersion value="4.0.1" />
  <mapping>
    <identity value="rim" />
    <uri value="http://hl7.org/v3" />
    <name value="RIM Mapping" />
